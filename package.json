    "@types/node": "^20.11.0",
    "@types/react": "^18.3.0",
    "@types/react-dom": "^18.3.0",
    "autoprefixer": "^10.4.22",
    "eslint": "^8.57.0",
    "eslint-config-next": "^14.2.0",
    "gh-pages": "^6.1.0",
    "postcss": "^8.5.6",
    "prettier": "^3.2.0",
